<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="http://www.sample-package.org">
	<valueType name="Денежный" base="xs:decimal" totalDigits="19" fractionDigits="4"/>
	<objectType name="Товар">
		<property name="Наименование" type="xs:string"/>
		<property name="Услуга" type="xs:boolean"/>
		<property xmlns:d3p1="http://www.sample-package.org" name="ЦенаПродажи" type="d3p1:Денежный"/>
	</objectType>
	<objectType name="Док">
		<property name="Номер" type="xs:string"/>
		<property name="Дата" type="xs:dateTime"/>
		<property xmlns:d3p1="http://www.sample-package.org" name="СтрокаТЧ" type="d3p1:СтрокаТЧ" lowerBound="0" upperBound="-1"/>
	</objectType>
	<objectType name="СтрокаТЧ">
		<property xmlns:d3p1="http://www.sample-package.org" name="Товар" type="d3p1:Товар"/>
		<property name="Количество" type="xs:decimal"/>
		<property xmlns:d3p1="http://www.sample-package.org" name="Цена" type="d3p1:Денежный"/>
		<property xmlns:d3p1="http://www.sample-package.org" name="Сумма" type="d3p1:Денежный"/>
	</objectType>
</package>